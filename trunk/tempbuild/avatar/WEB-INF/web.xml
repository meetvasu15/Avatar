<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
	<display-name>Avatar</display-name>
	<description>ICE AVATAR</description>

	<servlet>
		<servlet-name>GetToDoItem</servlet-name>
		<servlet-class>com.asu.avatar.restservices.GetToDoItem</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>com.asu.avatar.restservices.LoginService</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>GetToDoItem</servlet-name>
		<url-pattern>/gettodoitem</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/authenticateUser</url-pattern>
	</servlet-mapping>

</web-app>